{
  "name": "graphql-babel-boilerplate",
  "version": "0.1.0",
  "description": "A GraphQL starter project",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "build": "babel src -s -D -d dist",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "repository": "andreoav/graphql-babel-boilerplate",
  "author": "Andreo Vieria <contato@andreovieira.com.br>",
  "license": "MIT",
  "dependencies": {
    "apollo-server": "^2.0.0-beta.4",
    "graphql": "^0.13.2",
    "lodash.merge": "^4.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/node": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-es2017": "^7.0.0-beta.49",
    "@babel/preset-stage-0": "^7.0.0-beta.49",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^2.6.0",
    "nodemon": "^1.9.2",
    "prettier": "^1.13.4"
  }
}
